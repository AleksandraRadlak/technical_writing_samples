    <shortdesc class="- topic/shortdesc ">Here you will find a list of keyboard shortcuts in CodeStreamX Studio.</shortdesc>
    <refbody class="- topic/body        reference/refbody ">
        <table class="- topic/table " colsep="1" frame="all" id="unique_5_Connect_42_table_ggb_rvj_1bc" nd:nd-id="table_ggb_rvj_1bc" rowsep="1">
            <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">File Operations</ph></title>
            <tgroup class="- topic/tgroup " cols="2">
                <colspec class="- topic/colspec " colname="col1" colnum="1" colwidth="1*" xtrc="colspec:1;10:65" xtrf="file:/C:/Users/hp/OneDrive/Pulpit/tech%20writing/CodeStreamX%20Studio%20-%20User%20Guide/codestreamx_studio_keyboard_shortcuts.dita" style="width:50%;"/>
                <colspec class="- topic/colspec " colname="col2" colnum="2" colwidth="1*" xtrc="colspec:2;11:65" xtrf="file:/C:/Users/hp/OneDrive/Pulpit/tech%20writing/CodeStreamX%20Studio%20-%20User%20Guide/codestreamx_studio_keyboard_shortcuts.dita" style="width:50%;"/>
                <thead class="- topic/thead ">
                    <row class="- topic/row "><entry class="- topic/entry " colname="col1" dita-ot:x="1" dita-ot:y="1">Operation</entry><entry class="- topic/entry " colname="col2" dita-ot:x="2" dita-ot:y="1">Shortcut</entry></row>
                </thead>
                <tbody class="- topic/tbody ">
                    <row class="- topic/row "><entry class="- topic/entry " colname="col1" dita-ot:x="1" dita-ot:y="2">New Project</entry><entry class="- topic/entry " colname="col2" dita-ot:x="2" dita-ot:y="2">Ctrl + N</entry></row>
                    <row class="- topic/row "><entry class="- topic/entry " colname="col1" dita-ot:x="1" dita-ot:y="3">Open Project</entry><entry class="- topic/entry " colname="col2" dita-ot:x="2" dita-ot:y="3">Ctrl + O</entry></row>
                    <row class="- topic/row "><entry class="- topic/entry " colname="col1" dita-ot:x="1" dita-ot:y="4">Save</entry><entry class="- topic/entry " colname="col2" dita-ot:x="2" dita-ot:y="4">Ctrl + S</entry></row>
                    <row class="- topic/row "><entry class="- topic/entry " colname="col1" dita-ot:x="1" dita-ot:y="5">Close Project</entry><entry class="- topic/entry " colname="col2" dita-ot:x="2" dita-ot:y="5">Ctrl + W</entry></row>
                </tbody>
            </tgroup>
        </table>
        <table class="- topic/table " colsep="1" frame="all" id="unique_5_Connect_42_table_vby_yvj_1bc" nd:nd-id="table_vby_yvj_1bc" rowsep="1">
            <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">Editing Commands</ph></title>
            <tgroup class="- topic/tgroup " cols="2">
                <colspec class="- topic/colspec " colname="col1" colnum="1" colwidth="1*" xtrc="colspec:3;41:65" xtrf="file:/C:/Users/hp/OneDrive/Pulpit/tech%20writing/CodeStreamX%20Studio%20-%20User%20Guide/codestreamx_studio_keyboard_shortcuts.dita" style="width:50%;"/>
                <colspec class="- topic/colspec " colname="col2" colnum="2" colwidth="1*" xtrc="colspec:4;42:65" xtrf="file:/C:/Users/hp/OneDrive/Pulpit/tech%20writing/CodeStreamX%20Studio%20-%20User%20Guide/codestreamx_studio_keyboard_shortcuts.dita" style="width:50%;"/>
                <thead class="- topic/thead ">
                    <row class="- topic/row "><entry class="- topic/entry " colname="col1" dita-ot:x="1" dita-ot:y="1">Operation</entry><entry class="- topic/entry " colname="col2" dita-ot:x="2" dita-ot:y="1">Shortcut</entry></row>
